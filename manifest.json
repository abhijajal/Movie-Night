{
	"name":"movieNight",
	"version":"1.0",
	"manifest_version":2,
	"permissions": [
    "https://*/*"
  ],
	"background":{
		"scripts":["jquery-3.5.1.min.js","background.js"]
	},
	"content_scripts":[
		{
			"matches":["<all_urls>"],
			"js":["jquery-3.5.1.min.js","content.js"],
			"all_frames": true

		}
	],
	"browser_action":{
		"default_popup": "popup.html",
		"default_title": "Title"

	}

}